<h1>Edit Task</h1>
<%= form_with(model: @task, local: true) do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.check_box :completed %>
    <%= form.label :completed %>
  </div>
  <div>
    <%= form.submit "Update Task" %>
  </div>
<% end %>